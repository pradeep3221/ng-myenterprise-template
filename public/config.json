{
  "appName": "My Enterprise App",
  "apiBaseUrl": "https://api.example.com/v1",
  "logLevel": "debug",
  "envName": "local",
  "analytics": {
    "enabled": false,
    "provider": "gtag",
    "measurementId": "G-XXXXXXX"
  },
  "auth": {
    "tokenStorageKey": "auth_token"
